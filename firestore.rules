
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Rule for collection group queries on 'dashboards'
    match /{path=**}/dashboards/{docId} {
      allow list: if request.auth != null;
    }

    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
